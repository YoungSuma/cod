//Прячем формы при клике мимо
$(document).on('click', function(e) {
    let elem = $(e.target);
    let parentDots = elem[0].closest('.dots-button');
    let parentFormGroup = elem[0].closest('.show-form');
    if(parentDots==null &&  parentFormGroup==null ){
        $('.show-form').removeClass('show-form');
        $('.active-point').removeClass('active-point');
    };

    
});



//*****************************ТОЧКИ



//Оборачиваем label в div
$('.uc-calc-design label.t-img-select__control').wrap('<div class="image-select-wrapper"></div>');



//Формируем слои из фото

//Слой для крыши
(function () {
    setTimeout(function(){
        let block = $('.uc-krysha');
        let total = block.find('.t-slds__items-wrapper').attr('data-slider-totalslides');
        let imageContainer = '';
        for(let i=1; i<=total;i++){
            let imageUrl = block.find('.t-slds__item[data-slide-index='+i+'] .t-slds__bgimg').attr('data-original');
            let imageTitle = block.find('.t-slds__caption[data-slide-caption='+i+'] .t-slds__title').text();
            imageContainer+=`<img class="t-img t-width t107__width t-width_12" data-image-ral="${imageTitle}" src="${imageUrl}" imgfield="img" alt="">`;
        };
        let mainContainer = `<div class="krysha-image new-layers">${imageContainer}</div>`;
        $('.uc-main-image .t107').append(mainContainer);
    }, 1500);
})();

//Слой для кирпича
(function () {
    setTimeout(function(){
        let block = $('.uc-kirpych');
        let total = block.find('.t-slds__items-wrapper').attr('data-slider-totalslides');
        let imageContainer = '';
        for(let i=1; i<=total;i++){
            let imageUrl = block.find('.t-slds__item[data-slide-index='+i+'] .t-slds__bgimg').attr('data-original');
            let imageTitle = block.find('.t-slds__caption[data-slide-caption='+i+'] .t-slds__title').text();
