        $('.uc-main-image .t107').append(mainContainer);
    }, 1500);
})();



let otkosyFlag = false;
let uglyFlag = false;

//Показываем выбранные слои
function showLayers(){
    
    //Изначально удаляем класс активности у всех слоёв
    $('.uc-main-image img').removeClass('show-layer');
    
    //Получаем выбранные опции
    
    //Крыша
    let kryshaColor =  $('input[name="крыша"]:checked').val();
    //Кирпич
    let kirpychColor =  $('input[name="кирпич"]:checked').val();
    //Швы
    let shvyColor = $('input[name="швы"]:checked').val();
    
    //Цоколь
    let cokolColor = $('input[name="цоколь"]:checked').val();
    
    //Откосы
    let otkosyType = $('input[name="откосы-тип"]').index( $('input[name="откосы-тип"]:checked'));
    
    //Если без откосов
    if(otkosyType==2){
        $('input[name="откосы-цвет"]').prop('checked', false);
        $('.t-input-group_ri.otkosy').addClass('disable-block');
        otkosyFlag = true;
    
    //Если откосы с молдингом или простые
    }else{
        $('.t-input-group_ri.otkosy').removeClass('disable-block');
        if(otkosyFlag) $('input[name="откосы-цвет"]:first').prop('checked', true);
        let otkosyColor = $('input[name="откосы-цвет"]:checked').val();    
        if(otkosyType==0) $('.otkosy-image img[data-image-ral="M-'+otkosyColor+'"]').addClass('show-layer'); 
        if(otkosyType==1) $('.otkosy-image img[data-image-ral="'+otkosyColor+'"]').addClass('show-layer'); 
        otkosyFlag = false;
    };
    
    //Углы
    
    let uglyType = $('input[name="углы-тип"]').index( $('input[name="углы-тип"]:checked'));
    
    //Если без углов
    if(uglyType==1){
        $('input[name="углы-цвет"]').prop('checked', false);
        $('.t-input-group_ri.ugly').addClass('disable-block');
        uglyFlag = true;
    
    //Если откосы с молдингом или простые
    }else{
        $('.t-input-group_ri.ugly').removeClass('disable-block');
        if(uglyFlag) $('input[name="углы-цвет"]:first').prop('checked', true);
        let uglyColor = $('input[name="углы-цвет"]:checked').val();    
        if(uglyType==0) $('.ugly-image img[data-image-ral="'+uglyColor+'"]').addClass('show-layer'); 
        if(uglyType==1) $('.ugly-image img[data-image-ral="'+uglyColor+'"]').addClass('show-layer'); 
        uglyFlag = false;
    };
    

    //Выводим крышу, кирпич, швы, цоколь
    $('.krysha-image img[data-image-ral="'+kryshaColor+'"]').addClass('show-layer'); 
    $('.kirpych-image img[data-image-ral="'+kirpychColor+'"]').addClass('show-layer'); 
    $('.shvy-image img[data-image-ral="'+shvyColor+'"]').addClass('show-layer'); 
    $('.cokol-image img[data-image-ral="'+cokolColor+'"]').addClass('show-layer'); 
    
    
};



setTimeout(function(){
    showLayers();
}, 1500);





//Отслеживаем переключение элементов в Формируем
$(document).on('change', '.uc-calc-design', function(){
    showLayers();
});

$(document).on('click', '.image-select-wrapper', function(){
    let elem = $(this);
    let wrapper = elem.closest('.t-img-select__container');
    wrapper.find('input[type="radio"]').prop('checked', false);
    elem.find('input[type="radio"]').prop('checked', true);
    showLayers();
});



});
