$('input[name="цоколь"]').closest('.t-input-group').addClass('cokol contol-section');


//Элементы управления
$('.uc-calc-design .contol-section').wrapAll('<div class="contol-wrapper"></div>');


//Форма
$('.uc-calc-design .t-input-group_tx, .uc-calc-design .t-input-group_nm, .uc-calc-design .t-input-group_ph, .uc-calc-design .t-input-group_in, .uc-calc-design .t-input-group_cb, .uc-calc-design .t-form__errorbox-middle, .uc-calc-design .t-form__submit').wrapAll('<div class="form-section"><div class="form-section-small"></div></div>');





