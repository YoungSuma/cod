//Крыша
let dots = `<div class="dots-roof-wrapper dots-button t-text"><span></span></div>`;
$('.construktor-wrapper').append(dots);
$('.krysha').wrapInner('<div class="wrap-group"></div>');

$(document).on('click', '.dots-roof-wrapper', function(){
    if( $(this).hasClass('active-point') ){
        $('.active-point').removeClass('active-point');    
        $('.show-form').removeClass('show-form');
    }else{
        $('.active-point').removeClass('active-point');    
        $('.show-form').removeClass('show-form');
            $(this).addClass('active-point');    
            $('.krysha').addClass('show-form');
    };
});


//Кирпич
dots = `<div class="dots-kirpych-wrapper dots-button t-text"><span></span></div>`;
