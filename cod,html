$(document).ready(function(){
    

//Создаём общий блок для картинки+заголовка+формы, чтобы сделать прилипание в моб версии
$('.uc-main-image, .uc-calc-design').wrapAll('<div class="construktor-wrapper"></div>');


//Разметка формы

//Компоновка откосы
$('input[name="откосы-тип"]').closest('.t-input-group').addClass('otkosy');
$('input[name="откосы-цвет"]').closest('.t-input-group').addClass('otkosy');
$('.otkosy').wrapAll('<div class="otkosy-section contol-section"></div>');

//Компоновка углы
$('input[name="углы-тип"]').closest('.t-input-group').addClass('ugly');
$('input[name="углы-цвет"]').closest('.t-input-group').addClass('ugly');
$('.ugly').wrapAll('<div class="ugly-section contol-section"></div>');


//Разметка

//Крыша
$('input[name="крыша"]').closest('.t-input-group').addClass('krysha contol-section');
//Кирпич
$('input[name="кирпич"]').closest('.t-input-group').addClass('kirpych contol-section');
//Швы
